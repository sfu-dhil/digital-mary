{# Library of SVG icons from Bootstrap Icons #}

{% macro get(name, description) %}
    {% if block(name) is defined %}
        <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-{{ name|replace({'_': '-'}) }}" fill="currentColor"
             xmlns="http://www.w3.org/2000/svg">
            <title>{{ description|default(name) }}</title>
            {{ block(name) }}
        </svg>
    {% endif %}
{% endmacro %}

